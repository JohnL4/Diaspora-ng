<?xml version="1.0"?>
<cluster xmlns="http://how-hard-can-it-be.com/diaspora"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="https://s3.amazonaws.com/JohnLuskPublic/cluster.xsd cluster.xsd"
         usesHighLowSlipstreams="true"
         >
  <starSystem id="Alpha" name="Alpha Name" technology="0" environment="2" resources="-1">
    <aspect>Gentlemen, I give you the Pencarrow Endeavor!</aspect>
    <aspect>You're not from around here, are you?</aspect>
    <notes>
      Arbitrary text, multi-line, multi-paragraph, possibly with some limited HTML markup, or possibly some markdown.
    </notes>
  </starSystem>
  <starSystem id="Bravo" name="Bravo Name" technology="-1" environment="-3" resources="-1">
  </starSystem>
  <starSystem id="Charlie" name="Charlie Name" technology="2" environment="-1" resources="0">
  </starSystem>
  <starSystem id="Delta" name="Delta Name" technology="2" environment="-1" resources="-1">
  </starSystem>

  <slipstream from="Alpha" to="Bravo" leave="HIGH" arrive="LOW"/> <!-- to: idref (limitation: max one occurrence of destination per system); leave/arrive: enumerations -->
  <slipstream from="Bravo" to="Charlie" leave="LOW" arrive="LOW"/>
</cluster>
